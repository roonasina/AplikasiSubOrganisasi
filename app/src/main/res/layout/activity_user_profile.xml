<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xiirpl306162636.aplikasisuborganisasi.UserProfile">

    <ImageView
        android:id="@+id/imageViewbasket"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/imageViewmac"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="50dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/basket" />

    <ImageView
        android:id="@+id/imageViewmac"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/imageViewmedsan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="50dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/mac" />

    <ImageView
        android:id="@+id/imageViewmedsan"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="96dp"
        android:layout_marginLeft="50dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/medsan" />

    <ImageView
        android:id="@+id/imageViewpalwaga"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/imageViewbasket"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp"
        android:layout_toEndOf="@+id/imageViewbasket"
        android:layout_toRightOf="@+id/imageViewbasket"
        android:src="@drawable/palwaga" />

    <ImageView
        android:id="@+id/imageViewpaskibra"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/imageViewpalwaga"
        android:layout_alignStart="@+id/imageViewpalwaga"
        android:layout_alignTop="@+id/imageViewmac"
        android:src="@drawable/paskibra" />

    <ImageView
        android:id="@+id/imageViewperpus"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/imageViewpaskibra"
        android:layout_alignStart="@+id/imageViewpaskibra"
        android:layout_alignTop="@+id/imageViewmedsan"
        android:src="@drawable/perpus" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Daftar Organisasi" />

    <TextView
        android:id="@+id/sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageViewbasket"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="New Text" />

</RelativeLayout>
